<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div class="picture_frame">
        <%= image_tag @image.file %>
      </div>

      <div id="image_controls">
        <%= link_to "Delete", 
                    image_path(@image), 
                    method: :delete,
                    class: "btn btn-danger form-control" %>
      </div>
    </div>
  </div>
</div>